{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev:remix": "remix watch",
    "dev:css": "npm run generate:css -- --watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "remix build && run-p \"dev:*\"",
    "generate:css": "tailwindcss -m -i ./styles/app.css -o ./app/styles/tailwind.css",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc -b",
    "wrangler": "wrangler pages dev ./public"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.2.0",
    "@remix-run/cloudflare-pages": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@remix-run/v1-meta": "^0.1.3",
    "@types/marked": "^4.0.8",
    "cross-env": "^7.0.3",
    "isbot": "^3.5.4",
    "marked": "^4.2.1",
    "marked-quotebacks": "^1.0.2",
    "partysocket": "0.0.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.2.0",
    "@remix-run/eslint-config": "^2.2.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "partykit": "0.0.34",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.2",
    "typescript": "^5.2.2",
    "wrangler": "^3.15.0"
  },
  "engines": {
    "node": ">=14"
  }
}
